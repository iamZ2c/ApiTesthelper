      <locationPatterns>
        <locationMatch>
          <location>
            <protocol>https</protocol>
            <host>api-b.dcarapi.com</host>
          </location>
          <enabled>true</enabled>
        </locationMatch>
      </locationPatterns>
    </hosts>
    <rules>
      <rewriteRule>
        <active>true</active>
        <ruleType>1</ruleType>
        <matchHeader></matchHeader>
